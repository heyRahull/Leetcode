vector<bool> kidsWithCandies(vector<int>& candies, int extraCandies) {
    
        vector<bool>v;
        int ans = *max_element(candies.begin(),candies.end());
        
        for(int i=0;i<candies.size();i++)
        {
            if(candies[i]+extraCandies >= ans)
                v.push_back(true);
            else
                v.push_back(false);
                
        }
        return v;
    }
